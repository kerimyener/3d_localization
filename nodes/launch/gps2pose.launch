<launch>
<node pkg="geodetic_utils" type="set_gps_reference_node" output="screen" name="set2gps">
	<remap from ="gps" to= "xsens/mt0/fix"/>
</node>
<node pkg="geodetic_utils" type="gps_to_pose_conversion_node" output="screen" name="gps2pose">
        <rosparam command="load" file="$(find nodes)/param/gps2pose.yaml" />
      	<remap from ="imu" to= "xsens/mt0/imu/data"/>
	<remap from="gps" to= "xsens/mt0/fix"/>
        <remap from="gps_pose" to= "gnss_pose"/>


</node>
</launch>
